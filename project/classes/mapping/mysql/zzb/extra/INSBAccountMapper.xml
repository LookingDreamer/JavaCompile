<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zzb.extra.entity">
	<!-- INSBAccount -->
	<select id="INSBAccount_insert" parameterType="com.zzb.extra.entity.INSBAccount" resultType="int">
		insert into INSBAccount
		(id,operator,createtime,modifytime,noti,agentid,status)
		values
		(#{id},#{operator},#{createtime},#{modifytime},#{noti},#{agentid},#{status})
	</select>
	<select id="INSBAccount_select" parameterType="Map" resultType="com.zzb.extra.entity.INSBAccount">
		select * from INSBAccount
		<where>
			<if test="agentid != null">
				and agentid = #{agentid}
			</if>
			<if test="status != null">
				and status = #{status}
			</if>
		</where>
	</select>
	<select id="INSBAccount_existAccountId" parameterType="String" resultType="Long">
		select count(1) from INSBAccount where id = #{accountid}
	</select>
    <select id="INSBAccount_selectById" parameterType="String" resultType="com.zzb.extra.entity.INSBAccount">
       	select * from INSBAccount where id = #{id}
    </select>
    <select id="INSBAccount_selectByAgentId" parameterType="String" resultType="com.zzb.extra.entity.INSBAccount">
       	select * from INSBAccount where agentid = #{agentid}
    </select>
	<select id="INSBAccount_deleteById" parameterType="String">
		delete from INSBAccount where id = #{id}
	</select>
	<select id="INSBAccount_updateById" parameterType="com.zzb.extra.entity.INSBAccount">
		update INSBAccount  set operator = #{operator},modifytime = #{modifytime},noti = #{noti},agentid = #{agentid},status = #{status} where id = #{id}
	</select>
	<select id="INSBAccount_selectCount" parameterType="Map" resultType="Long">
		select count(1) from INSBAccount
		<where>
			<if test="agentid != null">
				and agentid = #{agentid}
			</if>
			<if test="status != null">
				and status = #{status}
			</if>
		</where>
	</select>
	<select id="INSBAccount_selectOne" parameterType="Map" resultType="com.zzb.extra.entity.INSBAccount">
		select * from INSBAccount
		<where>
			<if test="agentid != null">
				and agentid = #{agentid}
			</if>
			<if test="status != null">
				and status = #{status}
			</if>
		</where>
		LIMIT 2
	</select>

</mapper>