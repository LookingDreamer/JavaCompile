<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zzb.mobile.entity">
	<select id="AppEpayInfo_querySignInfos"  parameterType="String" resultType="com.zzb.mobile.entity.AppEpayInfo">
     	select id,operator,createtime,modifytime,noti,name,bankcardno,idcardno,idcardaddress,phone,khprovince,khcity,jobNum from insbepayinfo where jobNum=#{jobNum}
	</select>

	
	<select id="AppEpayInfo_getProvince" resultType="Map">
    	select comcode as provincecode,comcodename from insbregion where parentid='0'
	</select>
	
	<select id="AppEpayInfo_getCity" parameterType="String" resultType="Map">
		select comcode as citycode,comcodename from insbregion where comcodename !='åŽ¿' and parentid = #{parentid} 
	</select>	
	
	
		<select id="AppEpayInfo_insert" parameterType="com.zzb.mobile.entity.AppEpayInfo">
		insert into insbepayinfo
		(id,operator,createtime,modifytime,noti,name,phone,idcardno,khprovince,khcity,bankcardno,idcardaddress,jobNum,totalamount)
		values
		(#{id},#{operator},#{createtime},#{modifytime},#{noti},#{name},#{phone},#{idcardno},#{khprovince},#{khcity},#{bankcardno},#{idcardaddress},#{jobNum},#{totalamount})
	</select>
	
	
</mapper>