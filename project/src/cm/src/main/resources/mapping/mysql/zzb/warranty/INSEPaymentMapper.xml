<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zzb.warranty.model">
    <!-- INSEOrder  -->

    <select id="INSEPayment_insert" parameterType="com.zzb.warranty.model.INSEPayment" resultType="int">
        insert into INSEPayment
        (id,operator,createtime,modifytime, orderId, orderNo, transactionId, amount, channelId, payType, status)
        values
        (#{id},#{operator},#{createtime},#{modifytime}, #{orderId}, #{orderNo}, #{transactionId}, #{amount}, #{channelId}, #{payType}, #{status})
    </select>

    <select id="INSEPayment_selectById" parameterType="String" resultType="com.zzb.warranty.model.INSEPayment">
            select * from INSEPayment where id = #{id}
    </select>

    <update id="INSEPayment_updateById" parameterType="com.zzb.warranty.model.INSEPayment">
        update INSEPayment set status=#{status} where id=#{id}
    </update>

    <select id="INSEPayment_selectOne" parameterType="Map" resultType="com.zzb.warranty.model.INSEPayment">
        select * from INSEPayment
        <where>
            1=1
            <if test="transactionId!=null and transactionId!=''">
                and transactionId=#{transactionId}
            </if>
        </where>
    </select>

</mapper>