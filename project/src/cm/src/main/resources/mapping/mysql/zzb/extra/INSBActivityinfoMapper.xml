<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zzb.extra.entity">
	<!-- INSBActivityinfo -->
	<select id="INSBActivityinfo_insert" parameterType="com.zzb.extra.entity.INSBActivityinfo" resultType="int">
		insert into INSBActivityinfo
		(id,createtime,name,type,effectivetime,limited,endtime,status,conditions) 
		values
		(#{id},#{createtime},#{name},#{type},#{effectivetime},#{limited},#{endtime},#{status},#{conditions}) 
	</select>
	<select id="INSBActivityinfo_select" parameterType="Map" resultType="Map">
		select * from INSBActivityinfo
		<where>
			<if test="name != null">
				and name = #{name}
			</if>
			<if test="type != null">
				and type = #{type}
			</if>
			<if test="effectivetime != null">
				and effectivetime = #{effectivetime}
			</if>
			<if test="limited != null">
				and limited = #{limited}
			</if>
		</where>
	</select>
	<select id="INSBActivityinfo_selectById" parameterType="String" resultType="Map">
		select * from INSBActivityinfo where id = #{id}
	</select>
	<select id="INSBActivityinfo_deleteById" parameterType="String">
		delete from INSBActivityinfo where id = #{id}
	</select>
	<select id="INSBActivityinfo_updateById" parameterType="com.zzb.extra.entity.INSBActivityinfo">
		update INSBActivityinfo  set endtime = #{endtime},status = #{status},conditions = #{conditions}, name = #{name},type = #{type},effectivetime = #{effectivetime},limited = #{limited} where id = #{id}
	</select>
	<select id="INSBActivityinfo_selectCount" parameterType="Map" resultType="Long">
		select count(1) from INSBActivityinfo
		<where>
			<if test="name != null">
				and name = #{name}
			</if>
			<if test="type != null">
				and type = #{type}
			</if>
			<if test="effectivetime != null">
				and effectivetime = #{effectivetime}
			</if>
			<if test="limited != null">
				and limited = #{limited}
			</if>
		</where>
	</select>
	<select id="INSBActivityinfo_selectOne" parameterType="Map" resultType="com.zzb.extra.entity.INSBActivityinfo">
		select * from INSBActivityinfo
		<where>
			<if test="name != null">
				and name = #{name}
			</if>
			<if test="type != null">
				and type = #{type}
			</if>
			<if test="effectivetime != null">
				and effectivetime = #{effectivetime}
			</if>
			<if test="limited != null">
				and limited = #{limited}
			</if>
		</where>
		LIMIT 2
	</select>
</mapper>