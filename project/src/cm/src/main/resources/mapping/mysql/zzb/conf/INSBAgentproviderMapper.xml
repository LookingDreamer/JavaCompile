<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zzb.conf.entity">
	<!-- INSBAgentprovider -->
	<select id="INSBAgentprovider_insert" parameterType="com.zzb.conf.entity.INSBAgentprovider" resultType="int">
		insert into INSBAgentprovider
		(id,operator,createtime,modifytime,noti,agentid,providerid,status) 
		values
		(#{id},#{operator},#{createtime},#{modifytime},#{noti},#{agentid},#{providerid},#{status}) 
	</select>
	<select id="INSBAgentprovider_select" parameterType="Map" resultType="com.zzb.conf.entity.INSBAgentprovider">
		select * from INSBAgentprovider
		<where>
			<if test="agentid != null">
				and agentid = #{agentid}
			</if>
			<if test="providerid != null">
				and providerid = #{providerid}
			</if>
			<if test="status != null">
				and status = #{status}
			</if>
		</where>
	</select>
	<select id="INSBAgentprovider_selectOne" parameterType="Map" resultType="com.zzb.conf.entity.INSBAgentprovider">
		select * from INSBAgentprovider
		<where>
			<if test="agentid != null">
				and agentid = #{agentid}
			</if>
			<if test="providerid != null">
				and providerid = #{providerid}
			</if>
			<if test="status != null">
				and status = #{status}
			</if>
		</where>
		LIMIT 2
	</select>
	
	<select id="INSBAgentprovider_selectById" parameterType="String" resultType="com.zzb.conf.entity.INSBAgentprovider">
		select * from INSBAgentprovider where id = #{id}
	</select>
	<select id="INSBAgentprovider_selectByAgentId" parameterType="String" resultType="com.zzb.conf.entity.INSBAgentprovider">
		select * from INSBAgentprovider where agentid = #{agentId}
	</select>
	<select id="INSBAgentprovider_deleteById" parameterType="String">
		delete from INSBAgentprovider where id = #{id}
	</select>
	
	<select id="INSBAgentprovider_deleteByAgentId" parameterType="String">
		delete from INSBAgentprovider where agentid = #{agentId}
	</select>
	<select id="INSBAgentprovider_deleteByAgentIdProviderId" parameterType="Map">
		delete from INSBAgentprovider where agentid = #{agentId} and providerid=#{providerId}
	</select>
	<select id="INSBAgentprovider_updateById" parameterType="com.zzb.conf.entity.INSBAgentprovider">
		update INSBAgentprovider  set operator = #{operator},modifytime = #{modifytime},noti = #{noti},agentid = #{agentid},providerid = #{providerid},status = #{status} where id = #{id}
	</select>
	<select id="INSBAgentprovider_selectCount" parameterType="Map" resultType="Long">
		select count(1) from INSBAgentprovider
		<where>
			<if test="agentid != null">
				and agentid = #{agentid}
			</if>
			<if test="providerid != null">
				and providerid = #{providerid}
			</if>
			<if test="status != null">
				and status = #{status}
			</if>
		</where>
	</select>
</mapper>