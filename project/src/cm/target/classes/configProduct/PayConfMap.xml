		<!-- 支付类型 -->
		<payTypeMapping>
			<map cmvalue="01" typename="移动支付">mobile</map> <!-- 移动支付 -->
			<map cmvalue="02" typename="Web支付">web</map><!-- Web支付 -->
			<map cmvalue="03" typename="线下支付">offline</map><!-- 线下支付 -->
			<map cmvalue="04" typename="虚拟支付">visual</map><!-- 虚拟支付 -->
