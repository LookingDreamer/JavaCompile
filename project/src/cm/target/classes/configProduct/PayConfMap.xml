<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<baseSetting>
		<basePayUrl>http://10.251.144.80:8080/payment/restful/</basePayUrl>
		<payUrl>paymentService/pay</payUrl>
		<signkey>3A04F09282BB11E5AA85D4AE528C2144</signkey>
		<keyAttributes>channelId,channelName,payType,payTypeDesc,bizId,paySource,amount,notifyUrl,notifyType,areaCode,agentOrg,insOrg</keyAttributes>
		<paySource>zzb.system.core</paySource>
		<queryResult>paymentService/${bizId}/paymentResult</queryResult>
		<flowUrl>http://10.104.154.106/workflow/process/completeSubTask</flowUrl>
	</baseSetting>
	<confMapping>
	<!-- 证件类型 -->
		<certificateTypeMapping>
			<map cmvalue="0" typename="身份证">01</map> <!-- 身份证 -->
			<map cmvalue="1" typename="户口本">02</map> <!-- 户口本 -->
			<map cmvalue="2" typename="驾照">03</map> <!-- 驾照 -->
