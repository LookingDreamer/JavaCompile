			<map cmvalue="Yeepay" typename="易宝">yeepay</map><!-- 易宝 -->
			<map cmvalue="7" typename="支付测试通道">testPay</map><!-- 支付测试通道 -->
			<map cmvalue="8" typename="华安">huaan</map><!-- 华安 -->
