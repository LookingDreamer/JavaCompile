			<map cmvalue="7" typename="其他证件">99</map> <!-- 其他证件 -->
		</certificateTypeMapping>
		<!-- 银行卡类型 -->
		<bankCardMapping>
			<map cmvalue="cashcardflag" typename="借记卡/储蓄卡">01</map> <!-- 借记卡号/储蓄卡 -->
