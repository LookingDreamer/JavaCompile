	        <map cmvalue="06" typename="扫码支付">qrcode</map>
	        <map cmvalue="07" typename="签约支付">signed</map>
	        <map cmvalue="08" typename="快刷支付">quickpos</map>
	        <map cmvalue="09" typename="快速支付">quickpay</map>
		</payTypeMapping>
