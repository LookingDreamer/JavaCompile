<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zzb.conf.entity">
	<!-- INSBRenewalitem -->
	<select id="INSBRenewalitem_insert" parameterType="com.zzb.conf.entity.INSBRenewalitem" resultType="int">
		insert into INSBRenewalitem
		(id,operator,createtime,modifytime,noti,itemcode,itemname,iteminputtype,itemvaluelist,itemorder,isrequired) 
		values
		(#{id},#{operator},#{createtime},#{modifytime},#{noti},#{itemcode},#{itemname},#{iteminputtype},#{itemvaluelist},#{itemorder},#{isrequired}) 
	</select>
	<select id="INSBRenewalitem_select" parameterType="Map" resultType="com.zzb.conf.entity.INSBRenewalitem">
		select * from INSBRenewalitem
		<where>
			<if test="itemcode != null">
				and itemcode = #{itemcode}
			</if>
			<if test="itemname != null">
				and itemname = #{itemname}
			</if>
			<if test="iteminputtype != null">
				and iteminputtype = #{iteminputtype}
			</if>
			<if test="itemvaluelist != null">
				and itemvaluelist = #{itemvaluelist}
			</if>
			<if test="itemorder != null">
				and itemorder = #{itemorder}
			</if>
			<if test="isrequired != null">
				and isrequired = #{isrequired}
			</if>
		</where>
        order by itemorder asc
	</select>
	<select id="INSBRenewalitem_selectById" parameterType="String" resultType="com.zzb.conf.entity.INSBRenewalitem">
		select * from INSBRenewalitem where id = #{id}
	</select>
	<select id="INSBRenewalitem_deleteById" parameterType="String">
		delete from INSBRenewalitem where id = #{id}
	</select>
	<select id="INSBRenewalitem_updateById" parameterType="com.zzb.conf.entity.INSBRenewalitem">
		update INSBRenewalitem  set operator = #{operator},modifytime = #{modifytime},noti = #{noti},itemcode = #{itemcode},itemname = #{itemname},iteminputtype = #{iteminputtype},itemvaluelist = #{itemvaluelist},itemorder = #{itemorder},isrequired = #{isrequired} where id = #{id}
	</select>
	<select id="INSBRenewalitem_selectCount" parameterType="Map" resultType="Long">
		select count(1) from INSBRenewalitem
		<where>
			<if test="itemcode != null">
				and itemcode = #{itemcode}
			</if>
			<if test="itemname != null">
				and itemname = #{itemname}
			</if>
			<if test="iteminputtype != null">
				and iteminputtype = #{iteminputtype}
			</if>
			<if test="itemvaluelist != null">
				and itemvaluelist = #{itemvaluelist}
			</if>
			<if test="itemorder != null">
				and itemorder = #{itemorder}
			</if>
			<if test="isrequired != null">
				and isrequired = #{isrequired}
			</if>
		</where>
	</select>
	<select id="INSBRenewalitem_selectOne" parameterType="Map" resultType="com.zzb.conf.entity.INSBRenewalitem">
		select * from INSBRenewalitem
		<where>
			<if test="itemcode != null">
				and itemcode = #{itemcode}
			</if>
			<if test="itemname != null">
				and itemname = #{itemname}
			</if>
			<if test="iteminputtype != null">
				and iteminputtype = #{iteminputtype}
			</if>
			<if test="itemvaluelist != null">
				and itemvaluelist = #{itemvaluelist}
			</if>
			<if test="itemorder != null">
				and itemorder = #{itemorder}
			</if>
			<if test="isrequired != null">
				and isrequired = #{isrequired}
			</if>
		</where>
		LIMIT 1
	</select>
    <select id="INSBRenewalitem_selectAllByCodes" parameterType="Map" resultType="com.zzb.conf.entity.INSBRenewalitem">
       	select * from INSBRenewalitem
        <where>
            <if test="itemcodes != null and itemcodes.size!=0">
                itemcode in
                <foreach index="index" item="item" collection="itemcodes" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
        </where>
    </select>
</mapper>