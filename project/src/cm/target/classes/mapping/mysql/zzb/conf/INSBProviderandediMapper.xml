<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zzb.conf.entity">
	<!-- INSBProviderandedi -->
	<select id="INSBProviderandedi_insert" parameterType="com.zzb.conf.entity.INSBProviderandedi" resultType="int">
		insert into INSBProviderandedi
		(id,createtime,modifytime,noti,ediid,providerid,state,operator) 
		values
		(#{id},#{createtime},#{modifytime},#{noti},#{ediid},#{providerid},#{state},#{operator}) 
	</select>
	<select id="INSBProviderandedi_select" parameterType="Map" resultType="com.zzb.conf.entity.INSBProviderandedi">
		select * from INSBProviderandedi
		<where>
			<if test="ediid != null">
				and ediid = #{ediid}
			</if>
			<if test="providerid != null">
				and providerid = #{providerid}
			</if>
			<if test="state != null">
				and state = #{state}
			</if>
			<if test="operator != null">
				and operator = #{operator}
			</if>
		</where>
	</select>
	<select id="INSBProviderandedi_selectOne" parameterType="Map" resultType="com.zzb.conf.entity.INSBProviderandedi">
		select * from INSBProviderandedi
		<where>
			<if test="ediid != null">
				and ediid = #{ediid}
			</if>
			<if test="providerid != null">
				and providerid = #{providerid}
			</if>
			<if test="state != null">
				and state = #{state}
			</if>
			<if test="operator != null">
				and operator = #{operator}
			</if>
		</where>
		limit 2
	</select>
	<select id="INSBProviderandedi_selectById" parameterType="String" resultType="com.zzb.conf.entity.INSBProviderandedi">
		select * from INSBProviderandedi where id = #{id}
	</select>
	
	<select id="INSBProviderandedi_selectByPid" parameterType="String" resultType="Map">
		select b.id,b.ediname from insbproviderandedi a ,insbediconfiguration b WHERE a.ediid = b.id AND a.providerid=#{providerid}
	</select>
	
	<select id="INSBProviderandedi_deleteById" parameterType="String">
		delete from INSBProviderandedi where id = #{id}
	</select>
	<select id="INSBProviderandedi_deleterelation" parameterType="String">
		delete from INSBProviderandedi where ediid = #{ediid}
	</select>
	<select id="INSBProviderandedi_updateById" parameterType="com.zzb.conf.entity.INSBProviderandedi">
		update INSBProviderandedi  set modifytime = #{modifytime},noti = #{noti},ediid = #{ediid},providerid = #{providerid},state = #{state},operator = #{operator} where id = #{id}
	</select>
	<select id="INSBProviderandedi_selectCount" parameterType="Map" resultType="Long">
		select count(1) from INSBProviderandedi
		<where>
			<if test="ediid != null">
				and ediid = #{ediid}
			</if>
			<if test="providerid != null">
				and providerid = #{providerid}
			</if>
			<if test="state != null">
				and state = #{state}
			</if>
			<if test="operator != null">
				and operator = #{operator}
			</if>
		</where>
	</select>
</mapper>