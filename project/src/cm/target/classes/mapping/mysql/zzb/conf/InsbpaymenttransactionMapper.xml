<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zzb.mobile.entity">
	<select id="Insbpaymenttransaction_selectOne"  parameterType="String" resultType="com.zzb.mobile.entity.Insbpaymenttransaction">
     	select id,date,prfix,transaction,operator,createtime,modifytime,noti from insbpaymenttransaction where date=#{date}


	</select>
	<select id="Insbpaymenttransaction_insert"  parameterType="com.zzb.mobile.entity.Insbpaymenttransaction" resultType="int">
      insert into insbpaymenttransaction (
      	id,date,prfix,transaction,operator,createtime,modifytime,noti
      )
      values(
     	 #{id},#{date},#{prfix},#{transaction},#{operator},#{createtime},#{modifytime},#{noti}
      )    
	</select>	
	<select id="Insbpaymenttransaction_update"  parameterType="com.zzb.mobile.entity.Insbpaymenttransaction">
      update insbpaymenttransaction set id=#{id},date=#{date},prfix=#{prfix},transaction=#{transaction},operator=#{operator},createtime=#{createtime},modifytime=NOW(),noti=#{noti} where id=#{id}


	</select>
	
	
</mapper>