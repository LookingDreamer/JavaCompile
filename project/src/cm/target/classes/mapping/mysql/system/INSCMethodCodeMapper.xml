<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cninsure.system.entity">
	<insert  id="INSCMethodCode_insert" parameterType="com.cninsure.system.entity.INSCMethodCode" useGeneratedKeys="true" keyProperty="id">
		insert into INSCMethodCode
		(operator,createtime,noti,classname,method,methodname) 
		values
		(#{operator},#{createtime},#{noti},#{classname},#{method},#{methodname}) 
	</insert>
	<select id="INSCMethodCode_selectMethodNameByMethod" parameterType="Map" resultType="String">
		select methodname from  INSCMethodCode where method=#{method} and classname = #{classname} limit 1
	</select>
</mapper>